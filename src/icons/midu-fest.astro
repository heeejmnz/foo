<script>
  import gsap from "gsap";

  let timeline = gsap.timeline({ defaults: { duration: 0.4, ease: "back" } });
  let tl2 = gsap.timeline();

  const $a = document.querySelector("a");
  const letters = document.querySelectorAll(".letter");
  const numbers = document.querySelectorAll(".number");
  const fest = document.querySelectorAll(".FEST");

  const mouseoverAnimation = () => {
    timeline
      .to(letters, { y: -3, stagger: 0.05 })
      .to(fest, { y: -3, stagger: 0.05 }, "-=0.5")
      .to(numbers, { y: -3, stagger: 0.05 }, "-=0.5");
  };

  const mouseoutAnimation = () => {
    tl2
      .to(letters, { y: 0, stagger: 0.05, ease: "back", duration: 0.4 })
      .to(fest, { y: 0, stagger: 0.05, ease: "back", duration: 0.4 }, "-=0.5")
      .to(
        numbers,
        { y: 0, stagger: 0.05, ease: "back", duration: 0.4 },
        "-=0.5",
      );
  };

  $a?.addEventListener("mouseenter", mouseoverAnimation);
  $a?.addEventListener("mouseleave", mouseoutAnimation);
</script>

